body
  background #f3f3f3
  margin 0
  padding 0
  font-family Lato, Arial, Helvetica, sans-serif
a
  color #427bbf
  text-decoration none
  &:hover
    color #235898
    text-decoration underline
h1
  text-transform uppercase
  margin 0 0 16px 0
  padding-bottom 4px
  font-size 21px
  font-weight 300
img
  width auto
  max-width 100% !important
  display block
.wrapper
  width 100%
  max-width 1000px
  margin 0 auto
  padding 16px
  box-sizing border-box